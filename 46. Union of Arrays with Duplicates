class Solution {
    public static int findUnion(int a[], int b[]) {
        
        HashSet<Integer> st = new HashSet<>();
        for(int num:a){
            st.add(num);
        }
        
        for(int num : b){
            st.add(num);
        }
        
        int len = st.size();
        return len;
        
    }
}
